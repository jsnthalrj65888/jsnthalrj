# 起始URL
START_URL=https://8se.me/

# 爬取配置
MAX_DEPTH=3
MAX_PAGES=50

# 输出配置
OUTPUT_DIR=output
LOGS_DIR=logs

# 请求延迟（秒）
MIN_DELAY=1
MAX_DELAY=3

# 重试配置
MAX_RETRIES=3
TIMEOUT=30

# 并发配置
MAX_WORKERS=5

# 代理配置
USE_PROXY=false
PROXY_LIST_FILE=proxies.txt

# 浏览器配置
HEADLESS=true

# 爬虫策略
RESPECT_ROBOTS_TXT=true
SKIP_EXISTING=true

# 图片过滤
MIN_IMAGE_SIZE=10240
